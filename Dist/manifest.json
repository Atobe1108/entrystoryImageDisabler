{
    "manifest_version": 2,
    "name": "엔트리이야기 이미지 차단",
    "description": "엔트리이야기의 이미지를 차단해줍니다!",
    "version": "0.2",
    "permissions": ["activeTab"],
    "content_scripts": [
        {
          "matches": [
            "<all_urls>"
          ],
          "run_at": "document_start",
          "all_frames": true,
          "js": [
            "popup.js"
          ]
        }
    ]

    
}
